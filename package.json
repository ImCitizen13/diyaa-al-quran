{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "patch-package",
    "expo:dev": "EXPO_PACKAGER_PROXY_URL=https://$REPLIT_DEV_DOMAIN REACT_NATIVE_PACKAGER_HOSTNAME=$REPLIT_DEV_DOMAIN EXPO_PUBLIC_DOMAIN=$REPLIT_DEV_DOMAIN:5000 npx expo start --localhost",
    "server:dev": "NODE_ENV=development tsx server/index.ts",
    "expo:start:static:build": "npx expo start --no-dev --minify --localhost",
    "expo:static:build": "node scripts/build.js",
    "server:build": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=server_dist",
    "server:prod": "NODE_ENV=production node server_dist/index.js",
    "db:push": "drizzle-kit push",
    "start": "npx expo start",
    "lint": "npx expo lint",
    "lint:fix": "npx expo lint --fix",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo-google-fonts/amiri": "^0.4.1",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@shopify/react-native-skia": "2.2.12",
    "@stardazed/streams-text-encoding": "^1.0.2",
    "@tanstack/react-query": "^5.90.21",
    "@ungap/structured-clone": "^1.3.0",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "expo": "~54.0.33",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.13",
    "expo-crypto": "^15.0.8",
    "expo-font": "~14.0.11",
    "expo-glass-effect": "~0.1.9",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-local-authentication": "^17.0.8",
    "expo-location": "~19.0.8",
    "expo-notifications": "^0.32.16",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "express": "^5.2.1",
    "http-proxy-middleware": "^3.0.5",
    "pg": "^8.18.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "^1.18.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-skia-list": "^0.7.2",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.2",
    "react-native-worklets": "0.5.1",
    "tsx": "^4.21.0",
    "ws": "^8.19.0",
    "zod": "^4.3.6",
    "zod-validation-error": "^5.0.0"
  },
  "overrides": {
    "qs": "6.14.2",
    "tar": "7.5.7",
    "@isaacs/brace-expansion": "5.0.1",
    "esbuild": "^0.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@expo/ngrok": "^4.1.3",
    "@types/express": "^5.0.6",
    "@types/react": "~19.1.10",
    "babel-plugin-react-compiler": "^1.0.0",
    "drizzle-kit": "^0.31.9",
    "eslint": "^10.0.0",
    "eslint-config-expo": "~10.0.0",
    "patch-package": "^8.0.1",
    "typescript": "~5.9.3"
  },
  "private": true,
  "trustedDependencies": [
    "@shopify/react-native-skia"
  ]
}
